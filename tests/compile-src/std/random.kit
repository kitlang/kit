import kit.random;

function main() {
    var i = randomRange(1, 5);
    if i < 1 || i > 5 {
        printf("error: i = %i, should be 1-5\n", i);
    } else {
        puts("OK");
    }
    var f = random();
    if f < 0 || f >= 1 {
        printf("error: f = %.1f, should be 0-1\n", f);
    } else {
        puts("OK");
    }
}
