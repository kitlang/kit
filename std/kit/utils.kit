rules FormatString {
    (fmt(${_: Byte})) => "%hh";
    (fmt(${_: Short})) => "%h";
    (fmt(${_: Int})) => "%l";
    (fmt(${_: Long})) => "%ll";
    (fmt(${_: Float})) => "%f";
    (fmt(${_: Double})) => "%f";
}

